<div class="row body">
<div class="large-9 columns">	
<h1 id="about" class="about"><a href="">Show</a></h1>

<h2><%= @venue.name %></h2>
<p><%= @venue.city %></p>
<p><%= @venue.state %></p>

<h2>Events at this Venue</h2>
<% @events.each do |event| %>
<p><%= event.name %></p>
<% end %>


<%= link_to "Create Event", {:controller => "events", :action => "new", :venue_id => @venue.id}%><br>
<%= link_to 'Edit This Venue', edit_venue_path %><br>
<%= link_to 'Delete This Venue', venue_path, method: :delete %><br>
<%= link_to 'Back To Venues', venues_path %><br>

</div>
</div>